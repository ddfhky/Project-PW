<form>
    <h2>The result of the quiz</h2>
    <h2 style="color:red">Correct answers: <%= correctAnswers %>from<%= all %></h2><br>
    <% for (var i = 0 in intrebari ){ %>
        <label><% intrebari[i].intrebare</label> %></label><br>
        <% let c=0; %>
        <% for(var varianta of intrebari[i].variante){ %>
                <input type="radio" name="question<%= i %>" value="<%= c++ %>"/>
                <label for="question<%= i %>"><%= varianta %></label> 
                <br>
        <% }%>
        <label>Correct answer: </label>
        <input type="text" name="answer<%= i %>" value="<% intrebari[i].variante[intrebari[i].corect] %>" readonly></input>
        <br><br>
    <% } %>
</form>